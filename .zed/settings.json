{
  "lsp": {
    "rust-analyzer": {
      "initialization_options": {
        "checkOnSave": false,
        "lens": {
          "enable": false
        },
        "cargo": {
          "buildScripts": {
            "enable": true,
            "rebuildOnSave": false
          },
          "extraArgs": ["--offline"]
        },
        "procMacro": {
          "enable": true
        }
      }
    }
  }
}
